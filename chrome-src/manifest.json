{
  "name": "Simple Newsblur",
  "version": "0.1",
  "description": "Changes newsblur.com CSS",
  "permissions": ["tabs", "https://*.newsblur.com/*", "http://*.newsblur.com/*", "https://newsblur.com/*", "http://newsblur.com/*", "file:///*/*"],
    "content_scripts": [
    {
        "matches": [ "https://*.newsblur.com/*", "http://*.newsblur.com/*", "https://newsblur.com/*", "http://newsblur.com/*"],
        "js": ["start.js"],
        "all_frames": true,
        "run_at": "document_start"
    }
  ]
}
